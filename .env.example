# Sanity Configuration
# Sanity Project ID - find this in your Sanity project settings
NEXT_PUBLIC_SANITY_PROJECT_ID=your_project_id

# Sanity Dataset - usually 'production' or 'development'
NEXT_PUBLIC_SANITY_DATASET=production

# Sanity API Version - API version to use for Sanity client
NEXT_PUBLIC_SANITY_API_VERSION=2024-01-01

# Email Configuration
# Email User - email address for sending emails
EMAIL_USER=your_email@example.com

# Email Password - email password or app-specific password
EMAIL_PASS=your_email_password_or_app_password